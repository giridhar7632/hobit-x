{
  "expo": {
    "name": "Hobit App",
    "description": "Track your bite-sized habits",
    "slug": "hobit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "app.giridhar.hobit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "app.giridhar.hobit",
      "entitlements": {
        "aps-environment": "development"
      },
      "buildNumber": "1"
    },
    "android": {
      "package": "app.giridhar.hobit",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash.png",
          "dark": {
            "backgroundColor": "#151718"
          },
          "imageWidth": 200,
          "imageResizeMode": "contain"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9d684d52-edae-4c3d-b6bf-00d2608b5cb5"
      }
    },
    "owner": "giridhar7632"
  }
}
